        <type><ref refid="structshyft_1_1core_1_1land__type__fractions" kindref="compound">ec::land_type_fractions</ref></type>
        <definition>ec::land_type_fractions shyft::experimental::geo_cell_data_computer::compute_land_type_fractions</definition>
        <argsstring>(double cell_area, const multi_polygon &amp;cell, const multi_polygon &amp;rsv, const multi_polygon &amp;lake, const multi_polygon &amp;glacier, const multi_polygon &amp;forest) const</argsstring>
        <name>compute_land_type_fractions</name>
        <param>
          <type>double</type>
          <declname>cell_area</declname>
        </param>
        <param>
          <type>const <ref refid="namespaceshyft_1_1experimental_1ad4e1d347849eab005c8b30f83840766f" kindref="member">multi_polygon</ref> &amp;</type>
